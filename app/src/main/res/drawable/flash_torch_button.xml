<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true">
        <bitmap android:gravity="top|right|center_vertical|center_horizontal|center" android:src="@drawable/ic_flash_on_normal"/>
    </item>
    <item android:state_checked="false">
        <bitmap android:gravity="top|right|center_vertical|center_horizontal|center" android:src="@drawable/ic_flash_off_normal"/>
    </item>
</selector>
